{
  "env": {
      "browser": true,
      "mocha":true,
      "node":true,
      "jquery":true
  },
  "extends": "eslint:recommended",
  "rules": {
      "indent": [
          "error",
          2
      ],
      "linebreak-style": [
          "error",
          "windows"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ],
      "no-console":"off"
  }
}
